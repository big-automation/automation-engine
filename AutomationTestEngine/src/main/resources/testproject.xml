<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="testproject" name="testproject"
		class="org.bigtester.AutomationTestEngine.ProjectModel.TestProject">
		<property name="lSuite">
			<list>
				<ref bean="suite1" />
				<ref bean="suite2" />
				
			</list>
		</property>
	</bean>

	<!-- suite1 definition -->
	<bean id="suite1" name="suite1"
		class="org.bigtester.AutomationTestEngine.ProjectModel.TestSuite">
		<property name="suiteName" value="suite1" />
		<property name="lTestcase">
			<list>
				<ref bean="mytc1" />
				<ref bean="mytc2" />
				
			</list>
		</property>

	</bean>
	<bean id="mytc1" name="mytc1"
		class="org.bigtester.AutomationTestEngine.ProjectModel.TestCase">
		<property name="tcname" value="applicationContext.xml" />
	</bean>
	<bean id="mytc2" name="mytc2"
		class="org.bigtester.AutomationTestEngine.ProjectModel.TestCase">
		<property name="tcname" value="applicationContext2.xml" />
	</bean>

	<!-- suite2 definition -->
	<bean id="suite2" name="suite2"
		class="org.bigtester.AutomationTestEngine.ProjectModel.TestSuite">
		<property name="suiteName" value="suite2" />
		<property name="lTestcase">
			<list>
				<ref bean="mytc3" />
				<ref bean="mytc4" />
				
			</list>
		</property>
	</bean>
	<bean id="mytc3" name="mytc3"
		class="org.bigtester.AutomationTestEngine.ProjectModel.TestCase">
		<property name="tcname" value="applicationContext3.xml" />
	</bean>
	<bean id="mytc4" name="mytc4"
		class="org.bigtester.AutomationTestEngine.ProjectModel.TestCase">
		<property name="tcname" value="applicationContext4.xml" />
	</bean>
</beans>