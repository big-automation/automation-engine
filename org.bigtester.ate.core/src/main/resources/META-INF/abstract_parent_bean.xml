<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-4.0.xsd">
	
	<import resource="mywebdriver.xml" />
	<import resource="db-services.xml" />
	<bean id="_pageObjModel" name="_pageObjModel" abstract="true"
		class="org.bigtester.ate.model.page.AbstractPageModelBase">
		<property name="myWd">
			<ref bean="MyWebDriver"></ref>
		</property>
	</bean>
	<bean id="_pageObj" name="_pageObj" abstract="true" parent="_pageObjModel"
		class="org.bigtester.ate.model.page.page.AbstractPageObject">
	</bean>
	<bean id="_inputDataValueAbstract" name="_inputDataValueAbstract" abstract="true"
		class="org.bigtester.ate.model.data.AbstractDataValue">
		<property name="elementDataDao" ref="elementInputDataDao" />
	</bean>
	
	<!-- abstract beans, won't be initialized by Spring -->
	<bean id="_elementFindAbstract" name="_elementFindAbstract" abstract="true"
		class="org.bigtester.ate.model.page.elementfind.AbstractElementFind">
	</bean>
	<bean id="_elementActionAbstract" name="_elementActionAbstract" abstract="true" parent="_pageObjModel"
		class="org.bigtester.ate.model.page.elementaction.AbstractElementAction">
	</bean>
	<bean id="_abstractERAsserter" name="_abstractERAsserter"
		abstract="true"
		class="org.bigtester.ate.model.asserter.AbstractExpectedResultAsserter">
	</bean>

	<bean id="_abstractERValue" name="_abstractERValue" abstract="true"
		class="org.bigtester.ate.model.data.AbstractERValue">
		<property name="stepERDao" ref="StepERDao" />
	</bean>


</beans>
