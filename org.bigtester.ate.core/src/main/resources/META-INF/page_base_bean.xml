<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xmlns:aop="http://www.springframework.org/schema/aop"
	   xmlns:ate="http://www.bigtester.org/schema/ate"
	   xmlns:context="http://www.springframework.org/schema/context"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans 
	                       http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		                   http://www.springframework.org/schema/aop 
		                   http://www.springframework.org/schema/aop/spring-aop-4.0.xsd
		                   http://www.springframework.org/schema/context
		      	           http://www.springframework.org/schema/context/spring-context-4.0.xsd
		                   http://www.bigtester.org/schema/ate 
		                   http://www.bigtester.org/schema/ate/ate-2.0.xsd">
	
	<import resource="db-services.xml" />	
	
	<bean id="_DaoParent" class="org.bigtester.ate.model.data.dao.BaseDaoImpl" abstract="true" />	
	<bean id="elementInputDataDao" class="org.bigtester.ate.model.data.dao.ElementInputDataDaoImpl" />
	<bean id="StepERDao" class="org.bigtester.ate.model.data.dao.StepExpectedResultDaoImpl" parent="_DaoParent"/>
	<bean id="MyWebDriver2" class="org.bigtester.ate.model.page.atewebdriver.MyFirefoxDriver" />
		
	<ate:baseERValue id="mybaseERValue" stepERDao="StepERDao" abstract="true" />	
	<ate:basePageModel id = "myBasePageModel" myWebDriver="MyWebDriver2" abstract="true" />
	<ate:basePageObject id="myBasePageObject" parent="myBasePageModel" abstract="true" />
	<ate:baseElementAction id="baseElementAction" parent="myBasePageModel" abstract="true" />
	<ate:stepInputDataValueParent id="baseInputDataValue" elementDataDao="elementInputDataDao" abstract="true" />

</beans>
