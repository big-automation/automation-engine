<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="http://www.bigtester.org/schema/ate"
	        xmlns:tool="http://www.springframework.org/schema/tool" 
	        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	        xmlns:beans="http://www.springframework.org/schema/beans" 
	        xmlns:tns="http://www.bigtester.org/schema/ate"
	        targetNamespace="http://www.bigtester.org/schema/ate"
	        elementFormDefault="qualified" attributeFormDefault="unqualified">
<xsd:import namespace="http://www.springframework.org/schema/beans"
		    schemaLocation="http://www.springframework.org/schema/beans/spring-beans-4.0.xsd" />

	<!--************************* following for testproject ************************* -->

	<xsd:element name="TestProject">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="beans:identifiedType">
					<xsd:choice minOccurs="1" maxOccurs="unbounded">
						<xsd:element ref="TestSuite" />
					</xsd:choice>
					<xsd:attribute name="stepThinkTime" type="xsd:int" />
					<xsd:attribute name="globalInitXmlFile" type="xsd:string"
						use="required" />
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="TestSuite">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="beans:identifiedType">
					<xsd:choice minOccurs="1" maxOccurs="unbounded">
						<xsd:element ref="XmlTestCase" />
					</xsd:choice>
					<xsd:attribute name="suiteName" type="xsd:string" />
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="XmlTestCase">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="beans:identifiedType">
					<xsd:choice minOccurs="0" maxOccurs="1">
						<xsd:element ref="CaseDependencies" />
					</xsd:choice>
					<xsd:attribute name="testCaseFilePathName" type="xsd:string" />
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="CaseDependencies">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="beans:identifiedType">
					<xsd:choice minOccurs="1" maxOccurs="unbounded">
						<xsd:element ref="CaseDependency" />
					</xsd:choice>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="CaseDependency">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="beans:identifiedType">
					<xsd:attribute name="dependOnTestCaseID" type="xsd:string"
						use="required" />
					<xsd:attribute name="dependencyType" use="required">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:enumeration value="TestCaseStepDependency" />
								<xsd:enumeration value="TestCaseDataDependency" />
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:attribute>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="GenericSystemLogger">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="beans:identifiedType">
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:element name="TestDatabaseInitializer">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="beans:identifiedType">
					<!-- Here we re-use Spring elements and attributes -->
					<xsd:group ref="beans:beanElements" />
					<xsd:attributeGroup ref="beans:beanAttributes"></xsd:attributeGroup>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<!--************************* following for TestCase ************************* -->

	<xsd:complexType name="TestCaseType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<!-- Here we re-use Spring elements and attributes -->

				<xsd:choice maxOccurs="unbounded">
					<xsd:element ref="caseTypeService" minOccurs="0"
						maxOccurs="unbounded" />
					<xsd:element ref="stepTypeServiceReference" minOccurs="0"
						maxOccurs="unbounded" />
					<xsd:element ref="homeStep" minOccurs="1" maxOccurs="unbounded" />
					<xsd:element ref="elementStep" minOccurs="0" maxOccurs="unbounded" />
					<xsd:element ref="javaCodedStep" minOccurs="0" maxOccurs="unbounded" />
					<xsd:element ref="repeatStep" minOccurs="0" maxOccurs="unbounded" />
					<xsd:element ref="lastStep" minOccurs="1" maxOccurs="unbounded" />
				</xsd:choice>

				<xsd:attribute name="testCaseName" type="xsd:string"
					use="required" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="CaseTypeServiceType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<!-- Here we re-use Spring elements and attributes -->
				<xsd:group ref="beans:beanElements" />
				<xsd:attributeGroup ref="beans:beanAttributes" />
				<xsd:attribute name="testCaseName" type="xsd:string"
					use="required" />
				<xsd:attribute name="testCaseFileName" type="xsd:string"
					use="required" />
				<xsd:attribute name="parentTestCase" type="xsd:string"
					use="required">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type type="org.bigtester.ate.model.casestep.TestCase" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<!--************************* following for Test Step ************************* -->

	<xsd:complexType name="BaseTestStepType" abstract="true">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				
				<!-- Here we re-use Spring elements and attributes -->
<!-- 				<xsd:group ref="beans:beanElements" /> -->
				
<!-- 				<xsd:attributeGroup ref="beans:beanAttributes" /> -->
				<xsd:choice minOccurs="0" maxOccurs="1">
						<xsd:element ref="asserterReferences" />
					</xsd:choice>
				<xsd:attribute name="targetStep" type="xsd:boolean"
					use="required" />
				<xsd:attribute name="optionalStep" type="xsd:boolean" />
				<xsd:attribute name="correlatedOptionalStepsUtilInclusive"
					type="xsd:string" />
				<xsd:attribute name="stepName" type="xsd:string" use="required" />
				<xsd:attribute name="stepDescription" type="xsd:string" />
				
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:element name="asserterReferences">
		<xsd:complexType>
			<xsd:complexContent>
				<xsd:extension base="beans:identifiedType">
					<xsd:choice minOccurs="1" maxOccurs="unbounded">
						<xsd:element ref="ateXmlElementReference" />
					</xsd:choice>
				</xsd:extension>
			</xsd:complexContent>
		</xsd:complexType>
	</xsd:element>

	<xsd:complexType name="ElementStepType">
		<xsd:complexContent>
			<xsd:extension base="BaseTestStepType">
				<xsd:attribute name="elementActionDef" type="xsd:string"
					use="required">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.page.page.MyWebElement" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="pageObject" type="xsd:string">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.page.page.IPageObject" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

<xsd:complexType name="JavaCodedStepType">
		<xsd:complexContent>
			<xsd:extension base="BaseTestStepType">
				<xsd:attribute name="elementActionDef" type="xsd:string"
					use="required">
				</xsd:attribute>
				<xsd:attribute name="pageObject" type="xsd:string">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.page.page.IPageObject" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="StepTypeServiceRefType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<!-- <xsd:group ref="beans:beanElements"/> -->
				<!-- <xsd:attributeGroup ref="beans:beanAttributes"/> -->
				<xsd:attribute name="stepTypeServiceDefinitionID"
					type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.casestep.StepTypeService" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AteRefType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<!-- <xsd:group ref="beans:beanElements"/> -->
				<!-- <xsd:attributeGroup ref="beans:beanAttributes"/> -->
				<xsd:attribute name="ateXmlElementID"
					type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.asserter.IExpectedResultAsserter" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>


	<xsd:complexType name="StepTypeServiceDefType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">

				<xsd:choice maxOccurs="unbounded">

					<xsd:element ref="homeStep" minOccurs="0" maxOccurs="unbounded" />
					<xsd:element ref="elementStep" minOccurs="1" maxOccurs="unbounded" />
					<xsd:element ref="lastStep" minOccurs="0" maxOccurs="unbounded" />
				</xsd:choice>
				<xsd:attribute name="targetStep" type="xsd:boolean"
					use="required" />
				<xsd:attribute name="optionalStep" type="xsd:boolean" />
				<xsd:attribute name="correlatedOptionalStepsUtilInclusive"
					type="xsd:string" />
				<xsd:attribute name="stepName" type="xsd:string" use="required" />
				<xsd:attribute name="stepDescription" type="xsd:string" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="HomeStepType">
		<xsd:complexContent>
			<xsd:extension base="BaseTestStepType">
				<xsd:attribute name="elementActionDef" type="xsd:string">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.page.page.MyWebElement" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="pageObject" type="xsd:string"
					use="required">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.page.page.IHomePage" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="RepeatStepType">
		<xsd:complexContent>
			<xsd:extension base="BaseTestStepType">
				<xsd:attribute name="startStepName" type="xsd:string"
					use="required">
				</xsd:attribute>
				<xsd:attribute name="endStepName" type="xsd:string"
					use="required">
				</xsd:attribute>
				<xsd:attribute name="continueOnFailure" type="xsd:boolean"
					default="false">
				</xsd:attribute>
				<xsd:attribute name="numberOfIterations" type="xsd:int"
					use="required">
				</xsd:attribute>
				<xsd:attribute name="asserterValuesRemainSame" type="xsd:boolean"
					default="true">
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="LastStepType">
		<xsd:complexContent>
			<xsd:extension base="BaseTestStepType">
				<xsd:attribute name="elementActionDef" type="xsd:string">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.page.page.MyWebElement" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="pageObject" type="xsd:string"
					use="required">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.page.page.IPageObject" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="BaseERValueType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<!-- Here we re-use Spring elements and attributes -->
				<xsd:group ref="beans:beanElements" />
				<xsd:attributeGroup ref="beans:beanAttributes" />
				<xsd:attribute name="stepERDao" type="xsd:string" use="required">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.data.dao.StepExpectedResultDaoImpl" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="StepExpectedResultValueType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<xsd:group ref="beans:beanElements" />
				<xsd:attributeGroup ref="beans:beanAttributes" />
				<xsd:attribute name="dataValueID" type="xsd:string" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<!--************************* following for Test Page ************************* -->

	<xsd:complexType name="PageModelBaseType" abstract="false">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<!-- Here we re-use Spring elements and attributes -->
				<xsd:group ref="beans:beanElements" />
				<xsd:attributeGroup ref="beans:beanAttributes"></xsd:attributeGroup>
				<xsd:attribute name="myWebDriver" type="xsd:string"
					use="required">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.page.atewebdriver.IMyWebDriver" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>

		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="BasePageObjectType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<xsd:group ref="beans:beanElements" />
				<xsd:attributeGroup ref="beans:beanAttributes"></xsd:attributeGroup>
				<xsd:attribute name="dataFile" type="xsd:string" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="PageObjectType">
		<xsd:group ref="pageObjectGroup" minOccurs="1" maxOccurs="unbounded" />
	</xsd:complexType>

	<xsd:group name="pageObjectGroup">
		<xsd:choice>
			<xsd:element ref="lastPage" />
			<xsd:element ref="homePage" />
			<xsd:element ref="regularPage" />
		</xsd:choice>
	</xsd:group>

	<xsd:complexType name="LastpageType">
		<xsd:complexContent>
			<xsd:extension base="BasePageObjectType">
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="HomepageType">
		<xsd:complexContent>
			<xsd:extension base="BasePageObjectType">
				<xsd:attribute name="homeUrl" type="xsd:string"></xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="RegularPageType">
		<xsd:complexContent>
			<xsd:extension base="BasePageObjectType">
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="BaseElementActionType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<xsd:group ref="beans:beanElements" />
				<xsd:attributeGroup ref="beans:beanAttributes"></xsd:attributeGroup>
				<xsd:attribute name="dataValue" type="xsd:string">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.data.StepInputDataValue" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="PagePropertyCorrectnessAsserterType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<xsd:group ref="beans:beanElements" />
				<xsd:attributeGroup ref="beans:beanAttributes" />
				<xsd:attribute name="resultPage" type="xsd:string"
					use="required">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.page.page.IPageObject" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="stepERValue" type="xsd:string"
					use="required">
					<!-- <xsd:annotation> -->
					<!-- <xsd:appinfo> -->
					<!-- <tool:annotation kind="ref"> -->
					<!-- <tool:expected-type -->
					<!-- type="org.bigtester.ate.model.data.StepExpectedResultValue" /> -->
					<!-- </tool:annotation> -->
					<!-- </xsd:appinfo> -->
					<!-- </xsd:annotation> -->
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="PageElementExistenceAsserterType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<xsd:group ref="beans:beanElements" />
				<xsd:attributeGroup ref="beans:beanAttributes" />
				<xsd:attribute name="resultPage" type="xsd:string"
					use="required">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.page.page.IPageObject" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="stepERValue" type="xsd:string"
					use="required">
					<!-- <xsd:annotation> -->
					<!-- <xsd:appinfo> -->
					<!-- <tool:annotation kind="ref"> -->
					<!-- <tool:expected-type -->
					<!-- type="org.bigtester.ate.model.data.StepExpectedResultValue" /> -->
					<!-- </tool:annotation> -->
					<!-- </xsd:appinfo> -->
					<!-- </xsd:annotation> -->
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<!--************************* following for Test Data ************************* -->

	<xsd:complexType name="BaseInputDataValueType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<!-- Here we re-use Spring elements and attributes -->
				<xsd:group ref="beans:beanElements" />
				<xsd:attributeGroup ref="beans:beanAttributes" />
				<xsd:attribute name="strDataValue" type="xsd:string" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="InputDataValueParentType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<!-- Here we re-use Spring elements and attributes -->
				<xsd:group ref="beans:beanElements" />
				<xsd:attributeGroup ref="beans:beanAttributes" />
				<xsd:attribute name="elementDataDao" type="xsd:string"
					use="required">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.data.dao.ElementInputDataDaoImpl" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="StepInputDataValueType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<xsd:group ref="beans:beanElements" />
				<xsd:attributeGroup ref="beans:beanAttributes" />
				<xsd:attribute name="dataValueID" type="xsd:string" />
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="RunTimeDataHolderType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<xsd:group ref="beans:beanElements" />
				<xsd:attributeGroup ref="beans:beanAttributes"></xsd:attributeGroup>
				<xsd:attribute name="dataHolderType" use="required">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="ManualAssigned" />
							<xsd:enumeration value="PageSourceParsed" />
							<xsd:enumeration value="CaseServiceParsed" />
							<xsd:enumeration value="StepServiceParsed" />

							<xsd:enumeration value="RandomAlphaText" />
							<xsd:enumeration value="RandomNumbers" />
							<xsd:enumeration value="RandomTextNumbers" />
							<xsd:enumeration value="RandomEmail" />

						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
				<xsd:attribute name="dataValue" type="xsd:string"></xsd:attribute>
				<xsd:attribute name="pageHtmlLeftBoundry" type="xsd:string"></xsd:attribute>
				<xsd:attribute name="pageHtmlRightBoundry" type="xsd:string"></xsd:attribute>
				<xsd:attribute name="subCaseMappedDataHolderID" type="xsd:string"></xsd:attribute>
				<xsd:attribute name="indexOfSameTypeDataOnPage" type="xsd:int"></xsd:attribute>
				<xsd:attribute name="randomInputLength" type="xsd:int"></xsd:attribute>
				<xsd:attribute name="page" type="xsd:string">
					<xsd:annotation>
						<xsd:appinfo>
							<tool:annotation kind="ref">
								<tool:expected-type
									type="org.bigtester.ate.model.page.page.IPageObject" />
							</tool:annotation>
						</xsd:appinfo>
					</xsd:annotation>
				</xsd:attribute>
				<xsd:attribute name="dataType">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:enumeration value="Text" />
							<xsd:enumeration value="Number" />
							<xsd:enumeration value="Date" />
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="AutoIncrementalDataHolderType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<xsd:group ref="beans:beanElements" />
				<xsd:attributeGroup ref="beans:beanAttributes"></xsd:attributeGroup>
				<xsd:attribute name="startValue" type="xsd:int" use="required"></xsd:attribute>
				<xsd:attribute name="endValue" type="xsd:int"></xsd:attribute>
				<xsd:attribute name="pacing" type="xsd:int" use="required"></xsd:attribute>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<!--************************* following for Element Find ************************* -->

	<xsd:complexType name="GenericElementFindType">
		<xsd:complexContent>
			<xsd:extension base="beans:identifiedType">
				<xsd:attribute name="findByValue" type="xsd:string" />
