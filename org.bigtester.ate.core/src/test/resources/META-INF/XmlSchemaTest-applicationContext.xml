<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:ate="http://www.bigtester.org/schema/ate"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd 
	http://www.springframework.org/schema/aop 
	http://www.springframework.org/schema/aop/spring-aop-4.0.xsd
	http://www.bigtester.org/schema/ate http://www.bigtester.org/schema/ate/ate.xsd">

	
	<import resource="db-services.xml"/>
	
	<ate:fireFoxDriver id="MyWebDriver2" />
	<ate:basePageObject id="myAbstractPageObject">
		<property name="myWd">
			<ref bean="MyWebDriver2" />
		</property>
	</ate:basePageObject>
	
	<ate:homePage id="homepg" homeUrl="http://www.hellostop.com" parent="myAbstractPageObject"/>
	
	<ate:stepExpectedResultValue id="homeStepExpectedValue" dataValueID="1"/>
	
	<ate:pagePropertyCorrectnessAsserter id="homePagePropertyAsserter">
		<constructor-arg ref="homepg"/>
		<property name="stepERValue">
			<ref bean="homeStepExpectedValue"/>
		</property>
	</ate:pagePropertyCorrectnessAsserter>
	
	<ate:pagePropertyCorrectnessAsserter id="homeElementExistenceAsserter">
		<constructor-arg ref="homepg"/>
		<property name="stepERValue">
			<ref bean="homeStepExpectedValue"/>
		</property>
	</ate:pagePropertyCorrectnessAsserter>
	
		
		


</beans>